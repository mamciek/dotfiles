[core]
	symlinks = true
	autocrlf = input
[color]
	diff = auto
	branch = auto
	log = auto
	status = auto
	interactive = auto
[alias]
	gr = log --graph --full-history --all --color --pretty=tformat:"%x1b[31m%h%x09%x1b[32m%d%x1b[0m%x20%s%x20%x1b[33m(%an)%x1b[0m"
	l  = log --pretty=oneline
	br = branch -avv
	st = status
	ci = commit
	co = checkout
	cp = cherry-pick
	put = push origin HEAD:refs/publish/test
[push]
	default = simple
